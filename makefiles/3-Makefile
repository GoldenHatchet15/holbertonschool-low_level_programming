CC = gcc                             
SRC = main.c othername.c             
OBJ = $(SRC:.c=.o)                   
NAME = school                        
RM = rm -f  

all: $(NAME)

$(NAME): $(OBJ)
	$(CC) $^ -o $@

%.o: %.c
	$(CC) -c $< -o $@

clean:
	-$(RM) *~ $(NAME)

oclean:
	-$(RM) $(OBJ)

fclean: clean oclean

re: fclean all

.PHONY: clean oclean fclean re
